  </servlet>
  <servlet-mapping>
    <servlet-name>mySrvt7</servlet-name>
    <url-pattern>/weather</url-pattern>
  </servlet-mapping>  
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
