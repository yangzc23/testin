    <servlet-class>cn.servlet.test.EmpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/empList</url-pattern>
  </servlet-mapping>  
  
  <servlet>
    <servlet-name>def</servlet-name>
